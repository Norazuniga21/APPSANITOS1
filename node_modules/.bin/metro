#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@highcharts/highcharts-react-native/node_modules/metro/src/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../@highcharts/highcharts-react-native/node_modules/metro/src/cli.js" "$@"
  ret=$?
fi
exit $ret
